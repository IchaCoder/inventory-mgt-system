<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="418.0" prefWidth="564.0" stylesheets="@../../../style2.css" BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.group.InvoiceController">

      <children>
         <Label styleClass="printInvoice" text="Issued Goods">
         <VBox.margin>
            <Insets bottom="20.0" left="40.0" right="20.0" top="20.0" />
         </VBox.margin></Label>
      <VBox maxWidth="600.0" prefHeight="200.0" prefWidth="100.0" styleClass="invoiceMain">
         <VBox.margin>
            <Insets left="30.0" right="30.0" />
         </VBox.margin>
         <children>
            <VBox prefHeight="400.0" prefWidth="100.0" styleClass="invoiceChild">
               <children>
                  <HBox>
                     <VBox.margin>
                        <Insets bottom="10.0" left="30.0" right="30.0" top="30.0" />
                     </VBox.margin>
                  </HBox>
                  <HBox layoutX="40.0" layoutY="40.0" styleClass="billHeading">
                     <children>
                        <Region minWidth="250.0" prefHeight="27.0" />
                     </children>
                     <VBox.margin>
                        <Insets left="30.0" right="30.0" />
                     </VBox.margin>
                  </HBox>
                  <HBox>
                     <children>
                        <Region prefHeight="17.0" prefWidth="166.0" />
                     </children>
                     <VBox.margin>
                        <Insets left="30.0" right="30.0" />
                     </VBox.margin>
                  </HBox>
                  <HBox layoutX="40.0" layoutY="101.0">
                     <VBox.margin>
                        <Insets left="30.0" top="5.0" />
                     </VBox.margin>
                  </HBox>
                  <TableView fx:id="printInvoiceTable" minHeight="150.0" prefHeight="200.0" prefWidth="200.0">
                    <columns>
                      <TableColumn fx:id="name" prefWidth="75.0" text="Product Name" />
                      <TableColumn fx:id="id" prefWidth="75.0" text="id" />
                        <TableColumn fx:id="quantity" prefWidth="75.0" text="Quantity" />
                        <TableColumn fx:id="amount" minWidth="100.0" prefWidth="75.0" text="Amount (GHC)" />
                    </columns>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </TableView>
               </children>
            </VBox>
         </children>
         <padding>
            <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
         </padding>
      </VBox>
      </children>

</VBox>
